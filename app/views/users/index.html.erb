<%= render 'books/list', book: @book, user:@user %>
<h1>User</h1>
<table>
      <tr>
        <th>image</th>
        <th>name</th>
      </tr>
<% @users.each do |user| %>
    <tr>
      <td><%= attachment_image_tag @user, :profile_image, :fill, 100, 100, fallback: "no_image.jpg" %></td>
      <td><%= user.name %></td>
      <td><%= link_to "Show", user_path(user.id) %></td>
    </tr>

  <% end %>
</table>


